  end)

  aStructGui = {'AC Black Flag - Edit Abstergo Challenge','Abstergo_Challenge',550,300,false,320,150}
  aStructInfo = { {'Actual',0x18,vtDword,true}, {'Completed (= 0 &lt;~ 1)',0x58,vtByte,true}, {'Total',0x1C,vtDword,false},
                   {'Completed (= 3 &lt;~ 1)',0x0C,vtByte,false}, {'"ReActivate" (= 1)',0x0C,vtByte,false} }

  synchronize(function()
    f.IconTitle.Caption = string.format('%s', sMsg)
    f.IconInfo.Caption = string.format('[%d] Accomplishment Manager: %X+0x%X  |  VFT: {%X}',nRecord,addrChMgr,0x30,addrCheckPtr)
  end)

  local addrPerkList = readPointer(addrChMgr+0x30)
  local nCount = 0
  for i=0,nRecord-1 do
      local addrPerk = addrPerkList + 4 * i
      local nType = readInteger(readPointer(addrPerk)+0x04)
      local xHashId = readQword(readPointer(addrPerk)+0x0C)
      addrPerk = readPointer(readPointer(addrPerk))
      local nActual = readInteger(addrPerk+0x18)
      local nTotal =  readInteger(addrPerk+0x1C)
      --print(string.format('- %X  ~  %d / %d -',addrPerk,i,nRecord))

      local nID_loc = readInteger(addrPerk+0xC8)

      local sDescr = ''
      -- details: check out [Tools ~ Errors &amp; Settings ~ Maintenance scripts ~ Scripts to run...]
      if not pcall(function() sDescr = getChallengeDescr(xHashId) end) then sDescr = '' end

      local bPrint = false
      if (nChoice == 1 and nActual &lt; nTotal and sDescr ~= '') then bPrint = true end
      if (nChoice == 2 and sDescr ~= '') then bPrint = true end
      if (nChoice == 3 and nActual &lt; nTotal) then bPrint = true end
      if (nChoice == 4) then bPrint = true end

      if (bPrint) then
         nCount = nCount +1
         local sName = string.format('[%04d][%04X]: {%10X}  | %6d  %6d | %8X  &lt;%s&gt;',
                                 nCount, i*4, addrPerk, nActual, nTotal, xHashId, sDescr)
         --local sName = string.format('[%04d][%04X]: {%10X}  | %6d  %6d | %8X   %6d ~ &lt;%s&gt;',
         --                        nCount, i*4, addrPerk, nActual, nTotal, xHashId, nID_loc, sDescr)

         synchronize(function()
             f.ListBox.Items.add(sName)
         end)
         --print(string.format('[%03d]: {%08X} \t | \t (%d  \t %d)',i+1,addrPerk,nActual,nTotal))

         -- set all "challenges" as 'completed' ~ do not change unless at own risk !...
         local bCompleted = false
         if (bCompleted) then
            writeInteger(addrPerk+0x18,nTotal)
            writeBytes(addrPerk+0x58,0)
         end
      end
      --break
  end

end


