          <AssemblerScript>{ Game   : AC4BFSP.exe
  Version:
  Date   : 2018-05-06
  Author : Paul44

   Free Roaming...
  (using 'write coordinates' routine)
}

[ENABLE]
{$lua}

  -- could use this to chck VFT...
  getStaticAddr("8B 50 4C 56 8B CF FF D2 8B 0D",-8,"pFreeRoam1",2)
  local addrCamStransMgr = getAddressSafe("[[[[[pFreeRoam1]+0x8]+0x34]+0x54]]")
  --print(string.format('%X',addrCamStransMgr))
  writeInteger(getAddress("pFreeRoam1"),addrCamStransMgr)

  -- use fall damage vs godmode...
  nFallmr = readInteger(getAddress('nFallId'))
  local nId = readBytes(getAddress('bUseGodmode'))
  if nId == 1 then nFallmr = readInteger(getAddress('nGodId')) end


{$asm}
// Entity ~ see main script...
// code writes coordinates...
aobscanmodule(pCoord,$process,0F 28 48 30 8B 73 08 0F 29 0E)
registersymbol(pCoord)
alloc(newmem,$100,$process)

// struct: AC2FreeRoamingCamera +10: xyz coordinates
//aobscanmodule(FreeRoam,$process,0F 28 46 10 0F 29 45 D0 0F)
aobscanmodule(FreeCamera,$process,0F 29 46 10 0F 28 47 20 0F 29 46 20 D9 87 C8)
registersymbol(FreeCamera)
alloc(newmem2,$100,FreeCamera)

// yaw &amp; pitch...
aobscanmodule(Camera,$process,07 0F 29 46 40 0F 28 47 10 0F 29 46 50)
registersymbol(Camera)
alloc(newmem3,$100,Camera)

globalalloc(pFreeRoam,4,$process)
globalalloc(pCamera,4,$process)

// save current location x,y,z coordinates...
globalalloc(OrigCoord,12,"$process")
// init script variables
globalalloc(XYstepVal,4,"$process")
globalalloc(ZstepVal,4,"$process")
globalalloc(ZstepdropHeight,4,"$process")
globalalloc(nDropHeight,4,$process)

nDropHeight:
dd 42200000                      // 40.0 ~= 0x42200000

XYstepVal:
dd 3F800000                        // (float)1
ZstepVal:
dd 3F800000                        // (float)1
ZstepdropHeight:
dd 3F000000                        // (float)0.5


globalalloc(Xval,4,"$process")
globalalloc(Yval,4,"$process")
globalalloc(Zval,4,"$process")
globalalloc(bFreeRoam,1,"$process")
globalalloc(bFreeCam,1,"$process")
globalalloc(FOVCam,4,$process)
globalalloc(bCoordSave,1,"$process")
globalalloc(nFrameSpeed,4,"$process")
globalalloc(bCoordRestore,1,"$process")

bFreeRoam:
db 0

FOVCam:
dd 3F4F5C29                        // (float)0.81

// freeroam timer interval (smoother/faster = 50 - smoothest/fast = 20)
// choose between player or camera
nFrameSpeed:
dd 32                             // (int)50

// save_restore coordinates...
bCoordSave:
db 1
bCoordRestore:
db 0

// free cam roaming...
bFreeCam:
db 0

// camera roaming...
globalalloc(pStructCamOffset,4,$process)

pStructCamOffset:
dd 10


// free roaming...
label(code)
label(return)

// subroutines...
alloc(updateCrd,$100,$process)

updateCrd:
  // updated coordinates...
  mov ebx,[pStructCrdBase]
  add ebx,[pStructCrdOffset]
  mov edx,[XVal]
  mov [ebx],edx
  add ebx,4
  mov edx,[YVal]
  mov [ebx],edx
  add ebx,4
  mov edx,[ZVal]
  mov [ebx],edx
  ret

// Player roaming...
newmem:
{$try}
  push ebx
  mov ebx, eax
  sub ebx,0x10
  cmp [pEntity],ebx
  pop ebx
  jne code
  cmp byte ptr [bFreeRoam],1
  jne code
  push ebx
  push edx
  mov ebx,[pEntity]
  mov [pStructCrdBase],ebx
  // updated coordinates...
  call updateCrd
  mov ebx,[pStructCrdBase]
  movaps xmm1,[ebx+40]
  // restore original registers...
  pop edx
  pop ebx
  jmp code
{$except}
  inc [nCrash5]
code:
  movaps xmm1,[eax+30]
  mov esi,[ebx+08]
  jmp return

pCoord:
  jmp newmem
  db 90 90
return:


// Camera Roaming routine...
label(code2)
label(return2)

// subroutines...
alloc(updateCam,$100,$process)

updateCam:
  // updated coordinates...
  mov ebx,[pStructCrdBase]
  add ebx,[pStructCamOffset]
  mov edx,[XVal]
  mov [ebx],edx
  add ebx,4
  mov edx,[YVal]
  mov [ebx],edx
  add ebx,4
  mov edx,[ZVal]
  mov [ebx],edx
  ret

newmem2:
  push ebx
  push edx
  //cmp [esi+8],00000800
  //jne code2
  //mov [pFreeRoam],esi
  cmp byte ptr [bFreeCam],1
  jne code2
  mov ebx,[pFreeRoam1]
  test ebx,ebx
  je code2
  mov [pStructCrdBase],ebx
  // updated coordinates...
  call updateCam
  movaps xmm0,[esi+10]
  // restore original registers...
code2:
  pop edx
  pop ebx
  // setting FOV...
  push ebx
  mov ebx,[FOVCam]
  mov [esi+30],ebx
  pop ebx
@@:
  movaps [esi+10],xmm0
  movaps xmm0,[edi+20]
  jmp return2

FreeCamera:
  jmp newmem2
  nop 3
return2:

// camera...
label(code3)
label(return3)

newmem3:
  mov [pCamera],edi
code3:
  movaps xmm0,[edi+10]
  movaps [esi+50],xmm0
  jmp return3

Camera+05:
  jmp newmem3
  nop 3
return3:


[DISABLE]
bCoordSave:
db 0

pCoord:
  db 0F 28 48 30 8B 73 08

unregistersymbol(pCoord)
dealloc(newmem)

// camera roaming...
FreeCamera:
  db 0F 29 46 10 0F 28 47 20

unregistersymbol(FreeCamera)
dealloc(newmem2)

dealloc(updateCrd)
dealloc(updateCam)

// camera...
Camera+05:
  db 0F 28 47 10 0F 29 46 50

unregistersymbol(Camera)
dealloc(newmem3)

