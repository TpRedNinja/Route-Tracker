--function SetCoord()
    local IconHeight = 3.0
    -- Map waypoint does not have a Z-value, so we use a fixed Z-value...
    local addrMap = getAddress("[pMapWayp]")
    local bActive = readBytes(addrMap+0xA9)
    if (bActive == 0) then return end

    local addrHeight = getAddress("nDropHeight")
    local MapX = readFloat(addrMap+0x50)
    local MapY = readFloat(addrMap+0x54)
    local MapZ = readFloat(addrHeight)
    -- in case an icon is selected ~ not all of them return coordinates...
    local IconX = 0
    local IconY = 0
    local IconZ = 0
    local addrIcon = readInteger(readInteger(addrMap+0x88))
    if (addrIcon ~=0) then
        IconX = readFloat(addrIcon+0x40)
        IconY = readFloat(addrIcon+0x44)
        IconZ = readFloat(addrIcon+0x48)
    end
    -- print(string.format("[%s]",MapX))
    -- When selecting an icon, '0' values are returned for xy...
    local bMapUpdate = true
    local bIconUpdate = true

    if MapX ~= MapX or MapY ~= MapY then bMapUpdate = false end
    if MapX == 0 and MapY == 0 then return end
    --if IconX == 0 and IconY == 0 then return end
    if MapX == nil or MapY == nil or (MapX == 0 and MapY == 0) then
        bMapUpdate = false
    elseif math.floor(MapX) == -1 or math.floor(MapY) == -1 then
        bMapUpdate = false
    end
    if addrIcon == 0 then bIconUpdate = false end
    if IconX ~= IconX or IconY ~= IconY  or IconZ ~= IconZ then bIconUpdate = false end
    if IconX == nil or IconY == nil  or IconZ == nil then bIconUpdate = false end

    local zVal = IconZ+IconHeight
    if (readFloat(addrHeight) ~= 0) then zVal = math.max(zVal,readFloat(addrHeight)) end

    if bIconUpdate then
        writeFloat(addrEntity+0x8,zVal)
        writeFloat(addrEntity,IconX)
        writeFloat(addrEntity+0x4,IconY)
        -- clear waypoint...
        writeFloat(addrIcon+0x40,0)
        writeFloat(addrIcon+0x44,0)
        writeFloat(addrIcon+0x48,0)
        writeBytes(addrMap+0xA9,0)
    elseif bMapUpdate then
        writeFloat(addrEntity+0x8,MapZ)
        writeFloat(addrEntity,MapX)
        writeFloat(addrEntity+0x4,MapY)
        -- clear waypoint...
        writeFloat(addrMap+0x50,0)
        writeFloat(addrMap+0x54,0)
        writeBytes(addrMap+0xA9,0)
    end

    -- always enable noFallDamage/godmode...
    local mr = getAddressList().getMemoryRecordByID(nFallmr)
    mr.Active = true
--end


