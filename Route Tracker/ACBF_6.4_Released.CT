function CollectChallenges()
  local addrCheckPtr = readPointer(getAddress("pAccomplVFT"))
  if ( addrCheckPtr == 0 or addrCheckPtr == nil ) then
     local sText = "The 'check_Pointers' are not collected yet ! \n" ..
                   'Swap back ingame, and then return here...'
     local nType = mtInformation
     local nButton = mbOK
     messageDialog(sText, nType, nButton)
     return
  end

  sAOB = string.format('%X',addrCheckPtr)
  --print(sAOB)

  local sMsg = 'List Open Challenges'
  if (nChoice == 2) then sMsg = 'List All Challenges' end

  -- google: cheat engine Async exec Form invalid window handle
  synchronize(function()
      -- painting form...
      f = IconList
      f.Width = 600
      f.Show()
      f.Caption = 'AC4 - Abstergo Challenges...'
      local lBox = f.ListBox
      lBox.Clear()
      local lbTitle = f.IconTitle
      lbTitle.Caption = 'One moment please... processing...'
      local lbInfo = f.IconInfo
      lbInfo.Caption = ''
      local lbHead = f.IconHead
      lbHead.Caption = '   #    addrChallenge    |     Actual   Total '
      local lbMsg = f.IconMessage
      lbMsg.Caption = "See Readme on how to edit a Challenge.\n" .. "(you should only edit highlighted values)"
      --lbMsg.Visible = true
      local cbGod = f.GodMode
      --cbGod.Visible = false
      --cbGod.Checked = true
      f.GodMode.Caption  = 'God Mode:'
      -- show/hide info...
      f.Structure.Visible = true
      f.RecycleBin.Visible = false
      f.pCoord.Visible = false
  end)

  local scan = createMemScan(false)
  --scan.setOnlyOneResult(false)
  scan.firstScan(soExactValue,vtDword,rtRounded,sAOB,nil,0,0xffffffffffffffff,"-X-C+W",
                                      fsmNotAligned,nil,true,false,false,false)

  scan.waitTillDone()
  --scan.getOnlyResult()
  --print(string.format('- %X -',scan.Result))
  local scanList = createFoundList(scan)
  scanList.initialize()
  --print(scanList.Count)

  aStructGui = {'AC Black Flag - Edit Abstergo Challenge','Abstergo_Challenge',550,300,false,320,150}
  aStructInfo = { {'Actual',0x18,vtDword,true}, {'Completed (= 0 &lt;~ 1)',0x58,vtByte,true}, {'Total',0x1C,vtDword,false},
                   {'Completed (= 3 &lt;~ 1)',0x0C,vtByte,false}, {'"ReActivate" (= 1)',0x0C,vtByte,false} }

  synchronize(function()
    f.IconTitle.Caption = string.format('%s  -  {AccomplMgr: %X+0x%X}', sMsg, getAddress("[pAccomplishm]"), 0x30)
    f.IconInfo.Caption = string.format('[Total: %d] - VFT: {%X} -',scanList.Count,readPointer(scanList.Address[0]))
  end)

  local nCount = 0
  for i=0,scanList.Count-1 do
      local addrPerk = tonumber(scanList.Address[i],16)
      local nActual = readInteger(addrPerk+0x18)
      local nTotal =  readInteger(addrPerk+0x1C)

      local nID_loc = readInteger(addrPerk+0xC8)

      if ( (nChoice == 1 and nActual &lt; nTotal) or nChoice == 2) then
         nCount = nCount +1
         local sName = string.format('[%04d]: {%08X}      |    (%5d  \t %5d)',nCount,addrPerk,nActual,nTotal)
         synchronize(function()
             f.ListBox.Items.add(sName)
         end)
         --print(string.format('[%03d]: {%08X} \t | \t (%d  \t %d)',i+1,addrPerk,nActual,nTotal))

         -- set all "challenges" as 'completed' ~ do not change unless at own risk !...
         local bCompleted = false
         if (bCompleted) then
            writeInteger(addrPerk+0x18,nTotal)
            writeBytes(addrPerk+0x58,0)
         end
      end
      --break
  end
end


