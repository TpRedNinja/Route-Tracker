{$lua}
  local addrShipVFT = readPointer(readPointer(getAddress("pNavalShipPlayer")))
  sAOB = string.format('%X',addrShipVFT)
  --print(sAOB)

  -- verify that my_ship is still "active"...
  local pCheckVFT = getStaticAddr("55 8B EC 51 56 8B F1 8B 86 80 00 00 00 8B",0,"",3)
  --print(string.format('%X ~ %X',readPointer(addrShipVFT+0x1C),pCheckVFT))
  if ( pCheckVFT ~= readPointer(addrShipVFT+0x1C) ) then return end

  local al = getAddressList()
  -- script 'Ship Battles...'
  local mr = al.getMemoryRecordByID(14654)
  --mr.Options = '[]'

  local scan = createMemScan(false)
  --scan.setOnlyOneResult(false)
  scan.firstScan(soExactValue,vtDword,rtRounded,sAOB,nil,0,0xffffffffffffffff,"-X-C+W",
                                      fsmNotAligned,nil,true,false,false,false)

  scan.waitTillDone()
  --scan.getOnlyResult()


  local scanList = createFoundList(scan)
  scanList.initialize()
  --print(string.format('- %X -',scanList.Count-1))

  -- create subrecords of ships...
  -- nType: 0 = 'my ship'
  for i=0,scanList.Count-1 do
    --print(scanList.Address[i])
    local addrShip = tonumber(scanList.Address[i],16)
    local nType = readInteger(addrShip+0xD8)
    local validShip = readPointer(addrShip+0x4)

    if not (validShip == 0 or nType == 0) then
       local fTmpLevel = readFloat(readPointer(addrShip+0x60)+0x10)
       local fLevel = ''
       if ( fTmpLevel ~= nil ) then
          fLevel = string.format('%4.0f',fTmpLevel)
          fLevel = " (" .. string.gsub(fLevel,"%s+","") .. ")"
       end
       local sType = 'Enemy Ship'
       if (nType == 2) then sType = 'Friendly Ship' end
       sType = 'Ship Type ' .. nType

       local sFaction = readPointer(readPointer(readPointer(readPointer(addrShip+0xA4)+0x60))+0x80)
       local nFaction = readSmallInteger(readPointer(sFaction)+0x6)
       sFaction = readString(readPointer(readPointer(sFaction)+0x8),50)

       local mrSub = al.createMemoryRecord()
       local addrHealth = string.format('%X',addrShip+0xD0)
       local addrHealthMax = string.format('%4.0f',readFloat(addrShip+0xB4))
       addrHealthMax = string.gsub(addrHealthMax,"%s+","")
       --sFaction =  string.format(' {%X}',nFaction)

       local xLocID = readInteger(addrShip+0x164)
       local sDescr = aDescrShips[xLocID]
       if (sDescr == nil) then sDescr = '{' .. tostring(xLocID) .. '}' else sDescr = sDescr[1] end

       mrSub.setAddress(addrHealth)
       mrSub.setDescription(sDescr .. "   [" .. addrHealthMax .. "]" .. fLevel .. " " .. sFaction )
       mrSub.Type = 4
       mrSub.appendToEntry(mr)
       mrSub.DontSave = true             -- will still prompt 'to save'...
    --break
    end

  end

