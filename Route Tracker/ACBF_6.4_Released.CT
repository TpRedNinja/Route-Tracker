                writeFloat(addrOrigCoord+xOffsZ,0)
            end
        end
    end

    local mr = getAddressList().getMemoryRecordByID(nMr)
    if (mr == nil) then return end
    local timerMr = createTimer(getMainForm())
    timerMr.Interval = 100
    timerMr.OnTimer = function(timer)
